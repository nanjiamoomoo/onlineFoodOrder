<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.mapper.MenuItemMapper">
    <!-- findMenuItemByID(int menuItemId-->
    <select id="findMenuItemByID" resultType="menuItem">
        select * from menuitem where menuitemid = #{menuitemid}
    </select>

    <!--findMenusByRestaurantId(int restaurantId)-->
    <select id="findMenusByRestaurantId" resultType="menuItem">
        select * from menuitem where restaurantid = #{restaurantid}
    </select>
</mapper>